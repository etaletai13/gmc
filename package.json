{
  "name": "musicoin-daemon",
  "version": "1.5.1",
  "description": "a go-musicoin binary for node",
  "license": "MIT",
  "private": false,
  "homepage": "https://musicoin.org",
  "bugs": {
    "mail": "13@etaletai.com",
    "web": "https://github.com/etaletai13/gmc/issues"
  },
  "issues": "https://github.com/etaletai13/gmc/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/etaletai13/gmc.git"
  },
  "keywords": [
    "go-musicoin",
    "musicoin",
    "blockchain",
    "gmc",
    "wallet"
  ],
  "scripts": {
    "build": "./build/env.sh go run ./build/ci.go install",
    "clean": "mkdir -p bin && mv ./build/bin/* ./bin/",
    "clone": "git clone https://github.com/Musicoin/go-musicoin.git gomc",
    "copy": "mv gomc/* .",
    "install": "yarn clone && yarn copy && yarn patch && yarn build && yarn clean",
    "patch": "sed -i.tmp 's/1.7/1.10/g' ./build/ci.go",
    "test": "./build/env.sh go run ./build/ci.go test"
  }
}
